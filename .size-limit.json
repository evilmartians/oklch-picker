[
  {
    "name": "Files to download",
    "path": [
      "dist/**/*",
      "!dist/model-*.js",
      "!dist/*.ico",
      "!dist/*.webmanifest",
      "!dist/*.png"
    ],
    "limit": "65 KB"
  },
  {
    "name": "Base scripts to execute",
    "path": ["dist/*.js", "!dist/model-*.js"],
    "brotli": false,
    "limit": "90 KB"
  },
  {
    "name": "Scripts for 3D model",
    "path": "dist/model-*.js",
    "brotli": false,
    "limit": "500 KB"
  }
]
